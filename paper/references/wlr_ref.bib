	Issn = {1530-8995},
	Journal = {Annual Review of Cell and Developmental Biology},
	Month = {Oct},
	Note = {PMID: 28813177},
	Number = {1},
	Pages = {537-553},
	Publisher = {Annual Reviews},
	Title = {How Single-Cell Genomics Is Changing Evolutionary and Developmental Biology},
