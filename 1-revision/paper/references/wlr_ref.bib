	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Bias, Robustness And Scalability In Differential Expression Analysis Of Single-Cell RNA-Seq Data},
	Url = {https://www.biorxiv.org/content/early/2017/05/28/143289},
	Year = {2017},
	Bdsk-Url-1 = {https://www.biorxiv.org/content/early/2017/05/28/143289},
