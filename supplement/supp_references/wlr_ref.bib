	Keywords = {NF-κB; live-cell imaging; signaling dynamics; gene expression; RNA-seq; single-cell heterogeneity},
	Number = {4},
	Pages = {458--469.e5},
	Title = {Measuring Signaling and RNA-Seq in the Same Cell Links Gene Expression to Dynamic Patterns of NF-κB Activation},
	Ty = {JOUR},
